<?xml version='1.0' encoding='UTF-8'?>
<!-- Created with Cambalache 0.10.3 -->
<interface>
  <!-- interface-name main.ui -->
  <requires lib="gtk" version="4.6"/>
  <object class="GtkApplicationWindow" id="window">
    <property name="default-height">250</property>
    <property name="default-width">750</property>
    <property name="title">YTChannel2RSSFeed</property>
    <child>
      <object class="GtkStack" id="main_stack">
        <property name="transition-type">slide-left-right</property>
      </object>
    </child>
  </object>
  <object class="GtkBox" id="selection_box">
    <property name="name">selection_box</property>
    <property name="orientation">vertical</property>
    <child>
      <object class="GtkEntry" id="YT_URL">
        <property name="halign">center</property>
        <property name="margin-top">25</property>
        <property name="placeholder-text">YT channel URL ...</property>
        <property name="secondary-icon-name">emblem-important-symbolic</property>
        <property name="secondary-icon-tooltip-text">invalid URL</property>
        <property name="valign">center</property>
      </object>
    </child>
    <child>
      <object class="GtkButton" id="convert_button">
        <property name="halign">center</property>
        <property name="label">Convert</property>
        <property name="margin-top">10</property>
        <property name="valign">center</property>
      </object>
    </child>
  </object>
  <object class="GtkBox" id="output_box">
    <property name="name">output_box</property>
    <property name="orientation">vertical</property>
    <child>
      <object class="GtkLabel" id="output_label">
        <property name="halign">center</property>
        <property name="label">some YT URL</property>
        <property name="margin-top">50</property>
        <property name="selectable">True</property>
        <property name="valign">center</property>
      </object>
    </child>
    <child>
      <object class="GtkButton" id="restart_button">
        <property name="halign">center</property>
        <property name="label">enter new URL</property>
        <property name="margin-top">10</property>
        <property name="valign">center</property>
      </object>
    </child>
  </object>
</interface>
